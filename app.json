{
  "expo": {
    "name": "Liver Buddy",
    "slug": "liverbuddy",
    "version": "0.0.20",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "liverbuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.liverbuddy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/liver-buddy-transparent.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#bae7ff"
        }
      ],
      "expo-localization",
      "expo-sqlite",
      [
        "@howincodes/expo-dynamic-app-icon",
        {
          "liver-level-2": {
            "ios": "./assets/images/icon-level-2.png",
            "android": "./assets/images/icon-level-2.png"
          },
          "liver-level-3": {
            "ios": "./assets/images/icon-level-3.png",
            "android": "./assets/images/icon-level-3.png"
          },
          "liver-level-4": {
            "ios": "./assets/images/icon-level-4.png",
            "android": "./assets/images/icon-level-4.png"
          },
          "liver-level-5": {
            "ios": "./assets/images/icon-level-5.png",
            "android": "./assets/images/icon-level-5.png"
          },
          "liver-level-6": {
            "ios": "./assets/images/icon-level-6.png",
            "android": "./assets/images/icon-level-6.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "15f10e2b-78f7-40cf-9f27-186cc57aa3dc"
      }
    },
    "owner": "maxencedhx"
  }
}